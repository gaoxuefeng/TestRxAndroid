<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/BasicListLinearLayoutContainerV"
    android:layout_height="wrap_content"
    android:background="@color/WHITE"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/simple_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        android:textSize="18sp"
        android:textColor="#353535"
        android:text="title"
        android:layout_marginBottom="4dp"/>

    <TextView
        android:id="@+id/simple_sub_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#828282"
        android:textSize="14sp"
        android:layout_marginRight="14dp"
        android:text="subTitle"
        />

    <ImageView
        android:layout_gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/call_record_list_item_arrow"
        android:layout_marginRight="16dp"/>


</LinearLayout><!-- From: file:/Users/lvhonghe/project/ethank/ethank/trunk/client/yunge/Android/Par/CoyoteLib/res/layout/entry_tx_view.xml --><!-- From: file:/Users/lvhonghe/project/ethank/ethank/trunk/client/yunge/Android/Par/CoyoteLib/build/intermediates/bundles/debug/res/layout/entry_tx_view.xml -->